// Increase {{pascalCase name}}
export const add{{pascalCase name}} = () => ({
  type: 'ADD_{{upperCase name}}',
})

// Decrease {{pascalCase name}}
export const delete{{pascalCase name}} = () => ({
  type: 'DELETE_{{upperCase name}}',
})

export const edit{{pascalCase name}} = () => ({
  type: 'EDIT_{{upperCase name}}',
})
